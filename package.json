{
  "name": "epic-gallery",
  "version": "0.0.0",
  "description": "An epic gallery",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsup",
    "lint": "tsc",
    "semantic-release": "semantic-release --branches main",
    "release": "pnpm run build && changeset publish",
    "commit": "git-cz"
  },
  "files": [
    "dist/*"
  ],
  "keywords": [
    "react gallery"
  ],
  "author": "Abu Raihan",
  "license": "MIT",
  "dependencies": {
    "commitizen": "^4.3.0",
    "keypress-tool": "^2.1.0",
    "lucide-react": "^0.268.0",
    "swiper": "^10.1.0",
    "tailwind-merge": "^1.14.0",
    "tsup": "^7.2.0",
    "use-debounce": "^9.0.4"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.15",
    "cz-conventional-changelog": "^3.3.0",
    "postcss": "^8.4.27",
    "semantic-release": "^21.0.7",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.9"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/parrotfly/epic-gallery"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
